<div style="padding: 20px;">

<div id='nvbr'>
  <%= link_to 'JobbyJob', root_url %> &nbsp; &nbsp;
  <%= link_to 'New Jobby job', new_jobby_job_path, class: 'button button-primary' %>
</div>

<h4>Keep track of your job search</h4>

<table>
  <thead>
    <tr>
      <th  style="font-family:raleway;">Company</th>
      <th>Job title</th>
      <th>Rating</th>
      <th>City</th>
      <th>Resume sent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobby_jobs.each do |jobby_job| %>
      <tr>
        <td><%= link_to jobby_job.company, jobby_job %></td>
        <td><%= jobby_job.job_title %></td>
        <td><%= jobby_job.rating %></td>
        <td><%= jobby_job.city %></td>
        <td><%= jobby_job.resume_to_be_sent %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>
